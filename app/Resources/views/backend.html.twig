<!DOCTYPE html>
<html lang="fr">

<head>
  <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1, user-scalable=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="">

    <title>{% block title %}Backoffice ::{% endblock %} </title>
  
  {% block stylesheets %}
    <link rel="stylesheet" href="{{ absolute_url(asset('backoffice/assets/vendor_components/bootstrap/dist/css/bootstrap.min.css')) }}">
    <link rel="stylesheet" href="{{ absolute_url(asset('backoffice/css/master_style.css')) }}">
    <link rel="stylesheet" href="{{ absolute_url(asset('backoffice/css/skins/_all-skins.css')) }}">
    <link rel="stylesheet" href="{{ absolute_url(asset('backoffice/assets/fontawesome/css/all.min.css')) }}">
  {% endblock %}

	<!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
	<!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
	<!--[if lt IE 9]>
	<script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
	<script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
	<![endif]-->

</head>
<body class="hold-transition skin-blue sidebar-mini">
<div class="wrapper">

  <header class="main-header">
    <a href="#" class="logo">
      <span class="logo-mini">Backoffice</span>
      <span class="logo-lg"><b>BackOffice</b></span>
    </a>
    <nav class="navbar navbar-static-top">
      <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
        <span class="sr-only">Toggle navigation</span>
      </a>

      <div class="navbar-custom-menu">
        <ul class="nav navbar-nav">
          <li class="dropdown messages-menu">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
              <i class="fa fa-envelope"></i>
              <span class="label label-success">5</span>
            </a>
            <ul class="dropdown-menu scale-up">
              <li class="header">Message</li>
              <li>
                <ul class="menu inner-content-div">
                  <li><!-- start message -->
                    <a href="#">
                      <div class="pull-left">
                        <img src="{{ absolute_url(asset('backoffice/images/avatar.png'))}}" class="rounded-circle" alt="User Image">
                      </div>
                      <div class="mail-contnet">
                         <h4>
                          Lorem Ipsum
                          <small><i class="fa fa-clock-o"></i> 15 mins</small>
                         </h4>
                         <span>Lorem ipsum dolor sit amet, consectetur adipiscing elit.</span>
                      </div>
                    </a>
                  </li>
                  <!-- end message -->
                </ul>
              </li>
              <li class="footer"><a href="#">See all e-Mails</a></li>
            </ul>
          </li>
          {% if is_granted('IS_AUTHENTICATED_FULLY') %}
          <li class="dropdown user user-menu">
            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
              <img src="{{ absolute_url(asset('backoffice/images/avatar.png'))}}" class="user-image rounded-circle" alt="{{ app.user.username }}">
              <span class="d-none d-sm-inline-block">{{ app.user.username|title }}</span>
            </a>
            <ul class="dropdown-menu scale-up">
              <li class="user-body">
                <div class="row">
                  <div class="col text-left">
                    <a href="{{ absolute_url(path("fos_user_profile_edit")) }}">Profile</a>
                  </div>
                  <div class="col text-left">
                    <a href="{{ absolute_url(path('fos_user_security_logout'))}}">Deconnexion</a>
                  </div>
                </div>
                <!-- /.row -->
              </li>
            </ul>
          </li>
          {% endif %}
          
        </ul>
      </div>
    </nav>
  </header>
  
  <aside class="main-sidebar">
    <section class="sidebar">
      <div class="user-panel" style="background-image: url({{ absolute_url(asset('backoffice/images/user-info.jpg'))}});">
        {% if is_granted('IS_AUTHENTICATED_FULLY') %}
        <div class="image">
          <img src="{{ absolute_url(asset('backoffice/images/avatar.png'))}}" class="rounded-circle" alt="{{ app.user.username }}">
        </div>
        <div class="info">
          <p></p>
          <a href="#"><i class="fa fa-circle text-success"></i>Bonjour  {{ app.user.username|title }}</a>
          <p></p>
        </div>
        {% endif %}
        
      </div>
      <ul class="sidebar-menu" data-widget="tree">
        <li class="header">RUBRIQUES</li>
        <li>
          <a href="#">
            <i class="fa fa-tachometer-alt "></i> <span>Tableau de bord</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
        </li>
        <li class="treeview">
          <a href="#">
            <i class="fa fa-files-o"></i>
            <span>Profile</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <li><a href="{{ absolute_url(path('fos_user_profile_edit')) }}"><i class="fa fa-user-lock"></i> parametrage</a></li>
            <li><a href="#"><i class="fa fa-newspaper"></i> activites</a></li>
          </ul>
        </li>
        <li class="treeview">
          <a href="#">
            <i class="fa fa-files-o"></i>
            <span>Evenementiels</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <li><a href="{{ absolute_url(path('backend_actualite_index'))}}"><i class="fa fa-newspaper"></i> Actualités</a></li>
            <li><a href="{{ absolute_url(path('backend_buzz_index'))}}"><i class="fa fa-newspaper"></i> Buzz</a></li>
            <li><a href="{{ absolute_url(path('backend_event_index'))}}"><i class="fa fa-newspaper"></i> Events</a></li>
          </ul>
        </li>
        <li class="treeview ">
          <a href="#"><i class="fa fa-cogs"></i><span>Rubriques</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <li><a href="{{ absolute_url(path('backend_presentation_index'))}}"><i class="fa fa-file"></i> Presentation</a></li>
            <li><a href="{{ absolute_url(path('backend_service_index'))}}"><i class="fa fa-cogs "></i> Services</a></li>
            <li><a href="{{ absolute_url(path('backend_partenaire_index'))}}"><i class="fa fa-users "></i> Partenaires</a></li>
            <li><a href="{{ absolute_url(path('backend'))}}"><i class="fa fa-telegram"></i> Contact</a></li>
            <li><a href="{{ absolute_url(path('backend_photo_index'))}}"><i class="fa fa-picture-o"></i> Photothèque</a></li>
          </ul>
        </li>
        <li class="treeview ">
          <a href="#"><i class="fa fa-cogs"></i><span>Artistes</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <li><a href="{{ absolute_url(path('backend_artiste_index'))}}"><i class="fa fa-user"></i> Biographie</a></li>
            <li><a href="{{ absolute_url(path('backend_album_index'))}}"><i class="fa fa-file"></i> Albums</a></li>
          </ul>
        </li>
        <li class="treeview ">
          <a href="#"><i class="fa fa-cogs"></i><span>Concours</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <li><a href="{{ absolute_url(path('backend_concours_index'))}}"><i class="fa fa-user"></i> Presentation</a></li>
            <li><a href="{{ absolute_url(path('backend_reglement_index'))}}"><i class="fa fa-file"></i> Reglement</a></li>
            <li><a href="{{ absolute_url(path('backend_candidat_index'))}}"><i class="fa fa-users"></i> Candidat</a></li>
          </ul>
        </li>
        <li class="treeview ">
          <a href="#">
            <i class="fa fa-lock"></i>
            <span>Securite</span>
            <span class="pull-right-container">
              <i class="fa fa-angle-left pull-right"></i>
            </span>
          </a>
          <ul class="treeview-menu">
            <li><a href="{{ absolute_url(path('admin_user_index'))}}"><i class="fa fa-users"></i> Utilisateurs</a></li>
            <li><a href="#"><i class="fa fa-user-edit"></i> Redacteurs</a></li>
          </ul>
        </li>      
        
      </ul>
    </section>
  </aside>

  <div class="content-wrapper">
   {% block body %}{% endblock %}
  </div>
  
  <footer class="main-footer">
    <div class="pull-right d-none d-sm-inline-block">
      <b>Delrodie</b>
    </div>Copyright &copy; 2019 <a href="">ESTHETE</a>. Tous droits reservés.
  </footer>


</div>

    {% block javascripts %}
      <script src="{{ absolute_url(asset('backoffice/assets/vendor_components/jquery/dist/jquery.min.js')) }}"></script>
      <script src="{{ absolute_url(asset('backoffice/assets/vendor_components/popper/dist/popper.min.js')) }}"></script>
      <script src="{{ absolute_url(asset('backoffice/assets/vendor_components/bootstrap/dist/js/bootstrap.min.js')) }}"></script>
      <script src="{{ absolute_url(asset('backoffice/assets/vendor_components/jquery-slimscroll/jquery.slimscroll.min.js')) }}"></script>
      <script src="{{ absolute_url(asset('backoffice/assets/vendor_components/fastclick/lib/fastclick.js')) }}"></script>
      <script src="{{ absolute_url(asset('backoffice/js/template.js')) }}"></script>
      <script src="{{ absolute_url(asset('backoffice/js/demo.js')) }}"></script>
    {% endblock %}
	
</body>

</html>
