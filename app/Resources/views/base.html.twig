                        <li{% if (current_menu == 'event') %} class="current"{% endif %}><a href="{{ absolute_url(path('frontend_event_index')) }}">Events</a></li>
                        <li><a href="#">Contact</a></li>
                        <!--<li><a href="#">Concours</a></li>-->
                    </ul>
                    <div class="clearfix"></div>
                </div>
            </nav>
            <!-- Main Menu End-->

        </div>

