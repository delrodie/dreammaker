                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                </div>

                <div class="navbar-collapse collapse clearfix">
                    <ul class="nav navbar-nav navbar-right">
                        <li{% if (current_menu == 'accueil') %} class="current"{% endif %}><a href="{{ absolute_url(path('homepage')) }}">Accueil</a></li>
                        <li{% if (current_menu == 'actualite') %} class="current"{% endif %}><a href="{{ absolute_url(path('frontend_actualite_index')) }}">Actualités</a></li>
