{% extends 'base.html.twig' %}

{% block body %}
    <h1>Partenaire</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ partenaire.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ partenaire.nom }}</td>
            </tr>
            <tr>
                <th>Statut</th>
                <td>{% if partenaire.statut %}Yes{% else %}No{% endif %}</td>
            </tr>
            <tr>
                <th>Imagename</th>
                <td>{{ partenaire.imageName }}</td>
            </tr>
            <tr>
                <th>Imagesize</th>
                <td>{{ partenaire.imageSize }}</td>
            </tr>
            <tr>
                <th>Updatedat</th>
                <td>{% if partenaire.updatedAt %}{{ partenaire.updatedAt|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Slug</th>
                <td>{{ partenaire.slug }}</td>
            </tr>
            <tr>
                <th>Publiepar</th>
                <td>{{ partenaire.publiePar }}</td>
            </tr>
            <tr>
                <th>Modifiepar</th>
                <td>{{ partenaire.modifiePar }}</td>
            </tr>
            <tr>
                <th>Publiele</th>
                <td>{{ partenaire.publieLe }}</td>
            </tr>
            <tr>
                <th>Modifiele</th>
                <td>{{ partenaire.modifieLe }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('backend_partenaire_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('backend_partenaire_edit', { 'id': partenaire.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
